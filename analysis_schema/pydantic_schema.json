      "type": "object",
      "properties": {
        "Center": {
          "title": "Center",
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        "Radius": {
          "title": "Radius",
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "array",
              "items": [
